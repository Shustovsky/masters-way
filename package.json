  "dependencies": {},
  "devDependencies": {
    "concurrently": "^8.2.2",
    "husky": "9.0.11",
    "@openapitools/openapi-generator-cli": "^2.9.0"
  },
  "engines": {
    "node": "22.5.1",
    "pnpm": "8.15.2"
  },
  "packageManager": "pnpm@8.15.2"
