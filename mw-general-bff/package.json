{
  "name": "mw-general-bff",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "make build",
    "start": "nodemon --watch . --exec 'make start' --signal SIGTERM --ext go,json,yaml,sql,env,sh",
    "serve": "make start",
    "test:integration": "make test",
    "installAll": "make install",
    "swag": "make swag",
    "help": "make help",
    "api:generate": "pnpm concurrently \"pnpm run api:generate:general\" \"pnpm run api:generate:mw-general-bff\" \"pnpm run api:generate:mw-storage\" \"pnpm run api:generate:mw-notification\"",
    "api:generate:general": "openapi-generator-cli generate -g go -t ../go-common/openapi-templates/ -i ../mw-general/docs/swagger.json -o ./apiAutogenerated/general",
    "api:generate:mw-general-bff": "openapi-generator-cli generate -g go -t ../go-common/openapi-templates/ -i ../mw-general-bff/docs/swagger.json -o ./apiAutogenerated/mw-general-bff",
    "api:generate:mw-storage": "openapi-generator-cli generate -g go -t ../go-common/openapi-templates/ -i ../mw-storage/docs/swagger.json -o ./apiAutogenerated/mw-storage",
    "api:generate:mw-notification": "openapi-generator-cli generate -g go -t ../go-common/openapi-templates/ -i ../mw-notification/docs/swagger.json -o ./apiAutogenerated/mw-notification"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "3.1.4"
  }
}
