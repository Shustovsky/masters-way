{
  "name": "mw-chat-bff",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "make build",
    "start": "nodemon --watch . --exec 'make start' --signal SIGTERM --ext go,json,yaml,sql,env,sh",
    "serve": "make start",
    "test:integration": "make test",
    "installAll": "make install",
    "swag": "make swag",
    "help": "make help",
    "api:generate": "pnpm concurrently \"pnpm run api:generate:general\" \"pnpm run api:generate:chat\" \"pnpm run api:generate:mw-chat-websocket\" \"pnpm run api:generate:mw-chat-bff\"",
    "api:generate:general": "openapi-generator-cli generate -g go -t ../go-common/openapi-templates/ -i http://localhost:8000/docs/doc.json -o ./apiAutogenerated/general",
    "api:generate:chat": "openapi-generator-cli generate -g go -t ../go-common/openapi-templates/ -i http://localhost:8001/docs/doc.json -o ./apiAutogenerated/chat",
    "api:generate:mw-chat-bff": "openapi-generator-cli generate -g go -t ../go-common/openapi-templates/ -i http://localhost:7999/docs/doc.json -o ./apiAutogenerated/mw-chat-bff",
    "api:generate:mw-chat-websocket": "openapi-generator-cli generate -g go -t ../go-common/openapi-templates/ -i http://localhost:7994/docs/doc.json -o ./apiAutogenerated/mw-chat-websocket"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "3.1.4"
  }
}
